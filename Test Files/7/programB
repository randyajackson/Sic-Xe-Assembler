INIT_ARRAY		START 100

			LDA ZERO
			STA VALUE2ST
			
			LDA ZERO
			STA INDEX
			LDX INDEX

LOOP_START		LDA VALUE2ST
			ADD ZERO
			STA VALUE2ST

			LDX INDEX
			STA ALPHA,X

			LDA INDEX
			ADD THREE
			STA INDEX
			COMP THREEHUNDO

			JLT LOOP_START

VALUE2ST			RESW 1
INDEX			RESW 1
ALPHA			RESW 100

ZERO			WORD 0
THREE			WORD 3
THREEHUNDO		WORD 300

			END INIT_ARRAY
